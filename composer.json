{
    "name": "kiboko/native-magento-driver",
    "description": "A Magento driver for Akeneo PIM",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "kiboko/akeneo-connector-base": "~1.1",
        "doctrine/dbal": "2.4.*",
        "league/flysystem": "~1.0",
        "symfony/serializer": "~2.7",
        "akeneo/pim-community-dev": "~1.5"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@luni.fr"
        }
    ],
    "minimum-stability": "alpha",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git",
            "branch": "master"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lunistore/akeneo-connector-base.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git",
            "branch": "master"
        },
        {
            "type": "vcs",
            "url": "https://github.com/fzaninotto/Faker.git",
            "branch": "master"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\MagentoDriver\\": "src/MagentoDriver/",
            "Kiboko\\Component\\MagentoMapper\\": "src/MagentoMapper/",
            "Kiboko\\Component\\MagentoSerializer\\": "src/MagentoSerializer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\Kiboko\\Component\\MagentoDriver\\": "unit/Kiboko/Component/MagentoDriver/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
